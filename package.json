{
  "name": "talks",
  "description": "Slides from when I talked.",
  "version": "1.0.0",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "bootswatch": "^3.3.6"
  },
  "devDependencies": {
    "connect-livereload": "^0.5.3",
    "git-directory-deploy": "^1.1.0",
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.8.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-connect": "^0.10.1",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-cssmin": "^0.12.3",
    "grunt-contrib-jade": "^0.15.0",
    "grunt-contrib-less": "^1.0.1",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-contrib-watch": "^0.6.1",
    "less-plugin-autoprefix": "^1.4.2"
  },
  "engines": {
    "node": "^5.0.0",
    "npm": "^3.0.0"
  },
  "license": "ISC",
  "main": "Gruntfile.js",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production grunt build",
    "codeship-setup": "npm run npm-version && npm install && npm run build",
    "codeship-test": "npm run validate",
    "deploy": "git-directory-deploy --directory public --username lukekarrys --email lukekarrys@gmail.com --repo git@github.com:lukekarrys/talks.git --message='--skip-ci'",
    "npm-version": "npm install -g npm@`cat package.json | jq -r '.engines.npm'`",
    "start": "NODE_ENV=development grunt serve",
    "validate": "npm ls >/dev/null"
  }
